# Create a new Next.js project
npx create-next-app@latest manifestation-board
cd manifestation-board

# Install additional dependencies
npm install next-auth axios react-dnd recharts @prisma/client
npm install -D prisma typescript @types/react

# App Configuration
NEXT_PUBLIC_APP_NAME=Manifestation Board
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/manifestation_board"
JWT_SECRET="your_very_secret_key_here"
NEXTAUTH_SECRET="another_secret_key"

# Authentication
NEXTAUTH_SECRET=very_long_random_secret_key_here
NEXTAUTH_URL=http://localhost:3000

# OAuth Providers
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Financial API Integrations
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret
PLAID_ENV=sandbox

# Credit Bureau
EXPERIAN_API_KEY=your_experian_api_key

# Notification Services
SENDGRID_API_KEY=your_sendgrid_api_key

# Encryption
ENCRYPTION_KEY=long_random_encryption_key

# Logging and Monitoring
SENTRY_DSN=your_sentry_dsn
